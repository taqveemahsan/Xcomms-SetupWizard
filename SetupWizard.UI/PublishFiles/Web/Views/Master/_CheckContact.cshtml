@model XCommsWeb.Models.ContactResponse
<table class="table table-striped table-bordered table-hover table-scrollable" id="TblList">
    <thead>
        <tr>
            <th>
                api_status
            </th>
            <th>
                version
            </th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td class="center">
                @Model.meta.api_status
            </td>
            <td class="center">
                @Model.meta.version
            </td>
        </tr>
    </tbody>
</table>
<br />
<table class="table table-striped table-bordered table-hover table-scrollable" id="TblList">
    <thead>
        <tr>
            <th>
                input
            </th>
            <th>
                status
            </th>
            <th>
                wa_id
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.contacts)
        {
            <tr>
                <td class="center">
                    @item.input
                </td>
                <td class="center">
                    @item.status
                </td>
                <td class="center">
                    @item.wa_id
                </td>
            </tr>
        }

    </tbody>
</table>