@model IEnumerable<XCommsWeb.Models.PopQuizAnswerUserDetail>
@using ServerWeb.Helper;
@using XCommsWeb.Classes;

<table class="table table-striped table-bordered table-hover table-responsive" id="dtPopAlertAnaly" style="overflow:scroll" border="1">
    <thead>
        <tr>
            <th class="text-center"><i class="fa fa fa-user txt-color-blue fa-lg"></i> User Name</th>
            <th class="text-center"><i class="fa fa fa-user txt-color-blue fa-lg"></i> Display Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="text-center"> @item.UserName </td>
                <td class="text-center"> @item.DisplayName </td>
            </tr>
        }
    </tbody>
</table>

