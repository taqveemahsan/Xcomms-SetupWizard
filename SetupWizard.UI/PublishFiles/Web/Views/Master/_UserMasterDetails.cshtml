@model IEnumerable<XCommsWeb.Models.CYGNUS_Master_Users>

<table class="table table-striped table-bordered table-hover table-responsive" id="dtUserlist">
    <thead>
        <tr>
            <th class="text-center"><i class="fa fa-user txt-color-blue fa-lg"></i> Name</th>
            <th class="text-center"><i class="fa fa-user txt-color-blue fa-lg"></i> UserId</th>
            <th class="text-center"><i class="fa fa-envelope txt-color-blue fa-lg"></i> EmailId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="text-center">@item.Name</td>
                <td class="text-center">@item.UserId</td>
                <td class="text-center">@item.EmailId</td>
            </tr>
        }
    </tbody>
</table>
