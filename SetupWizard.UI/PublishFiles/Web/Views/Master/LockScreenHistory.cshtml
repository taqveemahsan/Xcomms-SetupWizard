                    <div class="col-md-12 mrgtop10">
                        <table id="dt_WallPaperHistory" class="table table-striped table-bordered table-hover" width="100%">
                            <thead>
                                <tr>
                                    <th class="text-center no-sort">
                                        <div class="smart-form text-center" style="width:25px;">
                                            <label class="checkbox">
                                                <input type="checkbox" name="checkbox" id="chkCheckAll">
                                                <i></i>
                                            </label>
                                        </div>
                                    </th>
                                    <th class="text-center"><i class="fa fa-desktop txt-color-blueDark fa-lg"></i> Lockscreen Title</th>
                                    <th class="text-center"><i class="fa fa-paperclip txt-color-blueDark fa-lg"></i> Lockscreen Position</th>
                                    <th class="text-center"><i class="fa fa-check-circle-o txt-color-blueDark fa-lg"></i> Status</th>
                                    <th class="text-center"><i class="fa fa-user txt-color-blueDark fa-lg"></i> Sent By</th>
                                    <th class="text-center"><i class="fa fa-calculator txt-color-blueDark fa-lg"></i> Sent Date/Time (EST)</th>
                                    <th class="text-center"><i class="fa fa-clock-o txt-color-blueDark fa-lg"></i> Schedule Status</th>
                                    <th class="text-center"><i class="fa fa-calendar txt-color-blueDark fa-lg"></i> Start Date</th>
                                    <th class="text-center"><i class="fa fa-calendar txt-color-blueDark fa-lg"></i> End Date</th>
                                    <th class="text-center"><i class="fa fa-paper-plane txt-color-blueDark fa-lg"></i> Resent</th>
                                    <th class="text-center"><i class="fa fa-check-square txt-color-blueDark fa-lg"></i> Active</th>
                                    <th class="text-center"><i class="fa fa-pencil-square txt-color-blueDark fa-lg"></i> Actions</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    Html.RenderPartial("_HistoryWallPaperListPartial", item);
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </article>
    </div>
