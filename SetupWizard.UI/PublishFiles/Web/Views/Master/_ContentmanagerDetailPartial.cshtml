@model IEnumerable<XCommsWeb.Models.PopAlertAnalyticDetail>

<table class="table table-striped table-bordered table-hover table-responsive" id="dtPopAlertAnaly" style="overflow:scroll" border="1">
    <thead>
        <tr>
            <th class="text-center"><i class="fa fa-user txt-color-blue fa-lg"></i> User Name</th>
            <th class="text-center"><i class="fa fa-bookmark txt-color-blue fa-lg"></i> Computer Name</th>
            @*<th class="text-center"><i class="fa fa-desktop txt-color-blue fa-lg"></i> Display Name</th>*@
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td class="text-center col-md-2">@item.Name</td>
                <td class="text-center col-md-3">@item.FullName</td>
                @*<td class="text-center col-md-4">@item.ComputerName</td>*@
            </tr>
        }
    </tbody>
</table>